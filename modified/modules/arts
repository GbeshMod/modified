@bot.on(admin_cmd(pattern="trumpsay (.*)"))
@bot.on(sudo_cmd(pattern="trumpsay (.*)", allow_sudo=True))
async def kraken(trumpsay):
    name = trumpsay.pattern_match.group(1)
    E = (
        f"**Donald Trump ‚û†‚û†{name} .\n\n**"
        "‚£ø‚£ø‚£ø‚£ø‚£ø‚£ø‚°ø‚†ø‚†õ‚†ã‚†â‚°â‚£â‚°õ‚£õ‚†ø‚£ø‚£ø‚£ø‚£ø‚£ø‚£ø‚£ø‚£ø\n"
        "‚£ø‚£ø‚£ø‚°ø‚†ã‚†Å‚†Ñ‚†Ñ‚†Ñ‚†Ñ‚†Ñ‚¢Ä‚£∏‚£ø‚£ø‚°ø‚†ø‚°Ø‚¢ô‚†ø‚£ø‚£ø‚£ø‚£ø\n"
        "‚£ø‚£ø‚°ø‚†Ñ‚†Ñ‚†Ñ‚†Ñ‚†Ñ‚°Ä‚°Ä‚†Ñ‚¢Ä‚£Ä‚£â‚£â‚£â‚†Å‚†ê‚£∂‚£∂‚£ø‚£ø‚£ø‚£ø\n"
        "‚£ø‚£ø‚°á‚†Ñ‚†Ñ‚†Ñ‚†Ñ‚†Å‚£ø‚£ø‚£Ä‚†à‚†ø‚¢ü‚°õ‚†õ‚£ø‚†õ‚†õ‚£ø‚£ø‚£ø‚£ø‚£ø\n"
        "‚£ø‚£ø‚°Ü‚†Ñ‚†Ñ‚†Ñ‚†Ñ‚†Ñ‚†à‚†Å‚†∞‚£Ñ‚£¥‚°¨‚¢µ‚£¥‚£ø‚£§‚£Ω‚£ø‚£ø‚£ø‚£ø‚£ø\n"
        "‚£ø‚£ø‚°á‚†Ñ‚¢Ä‚¢Ñ‚°Ä‚†Ñ‚†Ñ‚†Ñ‚†Ñ‚°â‚†ª‚£ø‚°ø‚†Å‚†ò‚†õ‚°ø‚£ø‚£ø‚£ø‚£ø‚£ø\n"
        "‚£ø‚°ø‚†É‚†Ñ‚†Ñ‚†à‚†ª‚†Ñ‚†Ñ‚†Ñ‚†Ñ‚¢ò‚£ß‚£Ä‚†æ‚†ø‚†∂‚†¶‚¢≥‚£ø‚£ø‚£ø‚£ø‚£ø\n"
        "‚£ø‚£∂‚£§‚°Ä‚¢Ä‚°Ä‚†Ñ‚†Ñ‚†Ñ‚†Ñ‚†Ñ‚†Ñ‚†ª‚¢£‚£∂‚°í‚†∂‚¢§‚¢æ‚£ø‚£ø‚£ø‚£ø‚£ø\n"
        "‚£ø‚°ø‚†ã‚†Ñ‚¢ò‚£ø‚£¶‚°Ä‚†Ñ‚†Ñ‚†Ñ‚†Ñ‚†Ñ‚†â‚†õ‚†ª‚†ª‚†∫‚£º‚£ø‚†ü‚†õ‚†ø‚£ø\n"
        "‚†ã‚†Å‚†Ñ‚†Ñ‚†Ñ‚¢ª‚£ø‚£ø‚£∂‚£Ñ‚°Ä‚†Ñ‚†Ñ‚†Ñ‚†Ñ‚¢Ä‚£§‚£æ‚£ø‚°Ä‚†Ñ‚†Ñ‚†Ñ‚¢π\n"
        "‚†Ñ‚†Ñ‚†Ñ‚†Ñ‚†Ñ‚†Ñ‚¢ª‚£ø‚£ø‚£ø‚£∑‚°§‚†Ñ‚†∞‚°Ü‚†Ñ‚†Ñ‚†à‚†õ‚¢¶‚£Ä‚°Ä‚°Ä‚†Ñ\n"
        "‚†Ñ‚†Ñ‚†Ñ‚†Ñ‚†Ñ‚†Ñ‚†à‚¢ø‚£ø‚†ü‚°ã‚†Ñ‚†Ñ‚†Ñ‚¢£‚†Ñ‚†Ñ‚†Ñ‚†Ñ‚†Ñ‚†à‚†π‚£ø‚£Ä\n"
        "‚†Ñ‚†Ñ‚†Ñ‚†Ñ‚†Ñ‚†Ñ‚†Ñ‚†ò‚£∑‚£ø‚£ø‚£∑‚†Ñ‚†Ñ‚¢∫‚£á‚†Ñ‚†Ñ‚†Ñ‚†Ñ‚†Ñ‚†Ñ‚†∏‚£ø\n"
        "‚†Ñ‚†Ñ‚†Ñ‚†Ñ‚†Ñ‚†Ñ‚†Ñ‚†Ñ‚†π‚£ø‚£ø‚°á‚†Ñ‚†Ñ‚†∏‚£ø‚°Ñ‚†Ñ‚†à‚†Å‚†Ñ‚†Ñ‚†Ñ‚£ø\n"
        "‚†Ñ‚†Ñ‚†Ñ‚†Ñ‚†Ñ‚†Ñ‚†Ñ‚†Ñ‚†Ñ‚¢ª‚£ø‚°á‚†Ñ‚†Ñ‚†Ñ‚¢π‚£ß‚†Ñ‚†Ñ‚†Ñ‚†Ñ‚†Ñ‚†Ñ‚†ò\n"
    )
    await edit_or_reply(trumpsay, E)


@bot.on(admin_cmd(pattern="chinasay (.*)"))
@bot.on(sudo_cmd(pattern="chinasay (.*)", allow_sudo=True))
async def kraken(ckmkb):
    name = ckmkb.pattern_match.group(1)
    F = (
        f"**üÖ≤üÖ∫üÖºüÖ∫üÖ± ‚û†‚û†{name} .\n\n**"

import asyncio
from telethon import events
from userbot.utils import admin_cmd, edit_or_reply, sudo_cmd
from platform import uname
from userbot import ALIVE_NAME, CMD_HELP, botnickname


DEFAULTUSER = str(ALIVE_NAME) if ALIVE_NAME else botnickname

USERID = bot.uid

mention = f"[{DEFAULTUSER}](tg://user?id={USERID})"



@bot.on(admin_cmd(pattern="carry (.*)"))
@bot.on(sudo_cmd(pattern="carry (.*)", allow_sudo=True))
async def kraken(carry):
    name = carry.pattern_match.group(1)
    await edit_or_reply(
        carry,
        f"**Carry ‚û†‚û†{name} .**\n\n                     ‚£§‚£∂‚£∂‚£∂‚£¶‚£§‚£Ñ‚°Ä\n‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚£∞‚£ø‚£ø‚£ø‚£ø‚£ø‚£ø‚£ø‚£ø‚£ø‚£¶‚°Ä\n‚†Ä‚†Ä‚†Ä‚¢Ä‚£æ‚£ø‚£ø‚£ø‚†ø‚†ø‚†ü‚†ª‚†ø‚¢ø‚£ø‚£ø‚£ø‚°Ü\n‚†Ä‚†Ä‚†Ä‚¢∞‚£ø‚£ø‚°ø‚†Ç‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä ‚†à‚†â‚¢ª‚°á \n‚†Ä‚†Ä‚†Ä‚†à‚†ø‚£ø‚£á‚£†‚†§‚†§‚†§‚¢§‚£Ä‚£§‚†§‚†§‚£∫‚°è \n‚†Ä‚†Ä‚†Ä‚†Ä‚†ê‚¢â‚£Ø‚†π‚£Ä‚£Ä‚£¢‚°∏‚†â‚¢è‚°Ñ‚£Ä‚£Ø‚†Å \n‚†Ä‚†Ä‚†Ä‚†Ä‚†°‚†Ä‚¢π‚£Ü‚†Ä‚†Ä‚†Ä‚£Ä‚°Ä‚°∞‚†Ä‚¢†‚†ñ‚†Ç \n‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†à‚†ô‚£ø‚£ø‚†Ä‚††‚†ö‚¢ã‚°Å‚†Ä‚°ú \n‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚¢∏‚†à‚†ô‚†¶‚£§‚£Ä‚£§‚£§‚°º‚†Å  \n‚†Ä‚†Ä‚†Ä ‚†Ä‚¢Ä‚°å‚†Ä‚†Ä‚†Ä‚†Ä ‚†â‚¢è‚°â  \n‚†Ä‚†Ä‚†Ä‚£Ä‚£¥‚£ø‚£∑‚£∂‚£§‚£§‚£§‚£¥‚£æ‚£∑‚£∂‚£¶‚°Ä \n‚¢Ä‚£¥‚£ø‚£ø‚£ø‚£ø‚£ø‚£ø‚£ø‚£ø‚£ø‚£ø‚£ø‚£ø‚£ø‚£ø‚£ø‚£Ñ \n‚†ö‚†õ‚†õ‚†õ‚†õ‚†õ‚†õ‚†õ‚†õ‚†õ‚†õ‚†õ‚†õ‚†õ‚†õ‚†õ‚†õ‚†õ",
    )


@bot.on(admin_cmd(pattern="ded (.*)"))
@bot.on(sudo_cmd(pattern="ded (.*)", allow_sudo=True))
async def kraken(ded):
    name = ded.pattern_match.group(1)
    await edit_or_reply(
        ded,
        f"{mention} --- {name}          \n„ÄÄ„ÄÄ„ÄÄ„ÄÄ„ÄÄ|"
        "\n„ÄÄ„ÄÄ„ÄÄ„ÄÄ„ÄÄ| \n"
        "„ÄÄ„ÄÄ„ÄÄ„ÄÄ„ÄÄ| \n"
        "„ÄÄ„ÄÄ„ÄÄ„ÄÄ„ÄÄ| \n"
        "„ÄÄ„ÄÄ„ÄÄ„ÄÄ„ÄÄ| \n"
        "„ÄÄ„ÄÄ„ÄÄ„ÄÄ„ÄÄ| \n"
        "„ÄÄ„ÄÄ„ÄÄ„ÄÄ„ÄÄ| \n"
        "„ÄÄ„ÄÄ„ÄÄ„ÄÄ„ÄÄ| \n"
        "„ÄÄÔºèÔø£Ôø£Ôºº| \n"
        "Ôºú ¬¥ÔΩ•  „ÄÄ |Ôºº \n"
        "„ÄÄ|„ÄÄÔºì„ÄÄ |‰∏∂Ôºº \n"
        "Ôºú „ÄÅÔΩ•„ÄÄ„ÄÄ|„ÄÄ„ÄÄÔºº \n"
        "„ÄÄÔººÔºøÔºøÔºè‚à™ _ ‚à™) \n"
        "„ÄÄ„ÄÄ„ÄÄ„ÄÄ„ÄÄ Ôºµ Ôºµ\n",
    )


@bot.on(admin_cmd(pattern="killer (.*)"))
@bot.on(sudo_cmd(pattern="killer (.*)", allow_sudo=True))
async def kraken(killer):
    name = killer.pattern_match.group(1)
    await edit_or_reply(
        killer,
        f"__**Commando **__{mention}          \n\n"
        "_/Ôπã\_\n"
        "(“Ç`_¬¥)\n"
        f"<,Ô∏ª‚ï¶‚ï§‚îÄ “â - - ----{name}\n"
        "_/Ôπã\_\n",
    )


A = (
    "‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä\n"
